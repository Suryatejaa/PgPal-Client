{
  "name": "pgpaalweb",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_APP_TYPE=base vite --mode development",
    "dev:owner": "cross-env VITE_APP_TYPE=owner vite --mode owner-development",
    "dev:tenant": "cross-env VITE_APP_TYPE=tenant vite --mode tenant-development",
    "dev:admin": "cross-env VITE_APP_TYPE=admin vite --mode admin-development",
    "dev:owner:prod-api": "cross-env VITE_APP_TYPE=owner vite --mode owner-development-prod-api",
    "dev:tenant:prod-api": "cross-env VITE_APP_TYPE=tenant vite --mode tenant-development-prod-api",
    "dev:admin:prod-api": "cross-env VITE_APP_TYPE=admin vite --mode admin-development-prod-api",
    "build": "vite build --mode production --outDir dist",
    "build:base": "cross-env VITE_APP_TYPE=base vite build --mode production --outDir dist",
    "build:owner": "cross-env VITE_APP_TYPE=owner vite build --mode owner-production --outDir dist-owner",
    "build:tenant": "cross-env VITE_APP_TYPE=tenant vite build --mode tenant-production --outDir dist-tenant",
    "build:admin": "cross-env VITE_APP_TYPE=admin vite build --mode admin-production --outDir dist-admin",
    "build:all": "npm run build:base && npm run build:owner && npm run build:tenant && npm run build:admin",
    "deploy:all": "echo 'Deployment is now handled by Vercel Git integration. Just push to your main branch.'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.8.1",
    "axios": "^1.9.0",
    "chart.js": "^4.4.9",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.0",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/js-cookie": "^3.0.6",
    "@types/jwt-decode": "^3.1.0",
    "@types/leaflet": "^1.9.18",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
