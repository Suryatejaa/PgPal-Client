{
  "version": 2,
  "builds": [
    {
      "src": "dist-base/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/dist-base/index.html" }],
  "env": {
    "VITE_API_URL": "https://api.purple-pgs.space",
    "VITE_WEBSOCKET_URL": "wss://ws.purple-pgs.space",
    "VITE_APP_TYPE": "base"
  }
}
